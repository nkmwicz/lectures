      "slide": true,
      "centered": true,
      "title": "Pull Factors",
      "text": [
        {
          "text": "Pull Factors are reasons drawing people to a place",
          "color": "lightgray"
        },
        {
          "text": "Examples:",
          "color": "darkslategray",
          "children": [
            {
              "text": "Gold Rush",
              "color": "darkslategray"
            },
            {
              "text": "A land of Religious Freedom",
              "color": "darkslategray"
            },
            {
              "text": "Nice Weather",
              "color": "darkslategray"
            }
          ]
        }
      ]
    },
